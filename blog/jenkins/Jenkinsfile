pipeline {
    agent any
    environment {
        RELEASE = "TEST"
    }
    stages {
        stage('Checkout') {
            steps {
            checkout scm 
            }
        }
         stage('Setup') { 
      steps {
        script {
                    sh """
            . .venv/bin/activate
            pip install -r requirements/test.txt
            """
        }
      }
    }
    }
}
