pipeline {
    agent any
    environment {
        RELEASE = "TEST"
    }
    stages {
        stage('Checkout') {
            steps {
            checkout scm 
             script {
                  bat  """
                    ./venv/Scripts/activate 
                   
            """
            
                }
            }
            
        }
        
         stage('Setup') { 
            steps {
                script {
                   bat  """
                    pip install -r requirements.txt 
                   
                     """
            
                    }
                }
         }
    }
}
